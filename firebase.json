{
  "functions": {
    "source": "functions"
  },
  "firestore": {
    "rules": "firestore.rules"
  },
"hosting": {
  "public": ".",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "rewrites": [
    {
      "source": "/api/**",
      "function": "placesProxy"
    },
    {
      "source": "!**/*.@(js|css|html|json|png|jpg|gif|svg|ico)",
      "destination": "/index.html"
    }
  ]
}
